s = input("Informe uma string: ")
#Inverter
def inverter_string(s):
    #Armazenar o resultado
    invertida = ""
    
    #Percorrer a string de trÃ¡s pra frente
    for i in range(len(s) - 1, -1, -1): 
        invertida += s[i]
    
    return invertida


print("String invertida:", inverter_string(s))